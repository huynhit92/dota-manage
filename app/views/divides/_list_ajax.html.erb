<%= form_for @q, url: divides_list_ajax_path, method: :get, html: {class: "form-horizontal"}, remote: true do |f| %>
  <%= f.label :name_cont %>
  <%= f.text_field :name_cont %>
  <%= f.submit "Search" %>
<% end %>
<% if @divides.present? %>
  <% @divides.each do |divide| %>
    <%= divide.name %>
    <button type="button" class="ajax_choose btn btn-default" data-value="<%= divide.id %>" data-target="rel_hero_divide_divide_id">選択</button>
  <% end %>
<% end %>